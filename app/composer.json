{
    "name": "dakword/wbseller",
    "description": "Wildberries API client",
    "keywords": ["wildberries", "api", "openapi", "php"],
    "license": "MIT",
    "type": "library",
    "require": {
        "php": "^7.4 || ^8.0",
        "guzzlehttp/guzzle": "^7.5",
        "slim/slim": "4.*",
        "slim/psr7": "^1.7",
        "guzzlehttp/psr7": "^2.7",
        "http-interop/http-factory-guzzle": "^1.2",
        "firebase/php-jwt": "^6.11",
        "vlucas/phpdotenv": "^5.6",
        "php-di/php-di": "^7.1",
      "ext-pdo": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5.0"
    },
    "scripts": {
        "test": "phpunit -v -c tests/phpunit.xml tests"
    },
    "autoload": {
        "psr-4": {
            "Dakword\\WBSeller\\": "src/",
            "App\\": "/",
            "App\\Middleware\\": "Middleware/",
            "App\\Controllers\\": "Controllers/",
            "App\\Services\\": "Services/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Dakword\\WBSeller\\Tests\\": "tests/",
            "App\\Tests\\": "tests/"
        }
    }
}